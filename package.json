{
  "name": "@schibstedspain/openads-google-adsense",
  "version": "1.0.0",
  "description": "OpenAds Google AdSense Connector",
  "main": "dist/",
  "scripts": {
    "cleanDistFolder": "rm -Rf ./dist",
    "prepublishOnly": "npm run cleanDistFolder && npm run build",
    "build": "npm run cleanDistFolder && babel src --ignore test --out-dir dist ",
    "test": "mocha --recursive --require babel-polyfill --compilers js:babel-register \"src/test/**/*.js\"",
    "lint": "sui-lint js",
    "coverage": "istanbul cover --report html _mocha -- ./src/*test --recursive --compilers js:babel-register",
    "coverage:ci": "istanbul cover _mocha -- ./src/*test --recursive --compilers js:babel-register && codecov"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/scm-spain/OpenAds-google-adsense"
  },
  "keywords": [
    "google",
    "adsense",
    "openads",
    "ads",
    "advertisement"
  ],
  "author": "The Mighty Ducks of Advertising",
  "license": "ISC",
  "devDependencies": {
    "@s-ui/lint": "^2.7.0",
    "babel-cli": "^6.24.0",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-sui": "1",
    "babel-register": "^6.24.0",
    "chai": "^4.0.2",
    "codecov": "^3.0.1",
    "istanbul": "^1.1.0-alpha.1",
    "jsdom": "^11.10.0",
    "mocha": "^3.4.2",
    "onchange": "^3.3.0",
    "sinon": "^2.3.4"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "babel": {
    "presets": [
      "sui"
    ]
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/@s-ui/lint/eslintrc.js"
    ]
  },
  "dependencies": {
    "loglevel": "^1.6.1"
  }
}
